$('#<%= raw(params[:update]) %>').html("<%= @meeting.status.titleize %>");
$('#<%= raw(params[:update]) %>').removeClass('available');
$('#<%= raw(params[:update]) %>').addClass('personal');
$('#<%= raw(params[:update]) + "-btn" %>').html("<%= escape_javascript(mark_available_button(@meeting.conference_date, @meeting, params[:update])) %>");
$('#<%= raw(params[:update]) %>').effect("highlight", {}, 1500);
